{
  "name": "octoagents",
  "version": "1.0.0",
  "description": "OpenCode Agentic Framework - Specialized agents, tools, and commands",
  "type": "module",
  "scripts": {
    "biome:check": "biome check . --write --unsafe --max-diagnostics 16384",
    "biome:format": "biome format . --write --max-diagnostics 16384",
    "biome:lint": "biome lint . --write --unsafe --max-diagnostics 16384",
    "build": "bun build src/install.ts --outdir . --target bun --outfile install.js"
  },
  "dependencies": {
    "@opencode-ai/plugin": "1.2.4",
    "zod": "4.3.6",
    "glob": "^11.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.1",
    "@types/bun": "^1.3.9",
    "@types/node": "^25.2.3",
    "typescript": "^5.9.3"
  }
}