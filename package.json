{
  "name": "octoagents",
  "version": "0.1.0",
  "description": "OpenCode Agentic Framework - Specialized agents, tools, and commands",
  "type": "module",
  "bin": {
    "octoagents-install": "./src/cli.ts"
  },
  "scripts": {
    "install-framework": "bun run src/cli.ts",
    "biome:check": "bunx biome check . --write --unsafe --max-diagnostics 16384",
    "biome:format": "bunx biome format . --write --max-diagnostics 16384",
    "biome:lint": "bunx biome lint . --write --unsafe --max-diagnostics 16384"
  },
  "dependencies": {
    "@opencode-ai/plugin": "1.2.10",
    "glob": "^13.0.6",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.4",
    "@types/bun": "^1.3.9",
    "@types/node": "^25.3.0",
    "typescript": "^5.9.3"
  }
}
